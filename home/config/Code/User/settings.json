    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "python.analysis.diagnosticMode": "workspace",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "pylance.insidersChannel": "daily",
  "editor.fontFamily": "Source Code Pro",
